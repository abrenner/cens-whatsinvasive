				android:layout_below="@+id/ImageView06"></TextView>
			<ImageView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:layout_centerInParent="true"
				android:layout_below="@+id/TextView08" android:id="@+id/ImageView07"
				android:src="@drawable/main_help_results"></ImageView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:padding="10px"
				android:textSize="@dimen/twozero_sp" android:textColor="@color/white"
				android:layout_below="@+id/ImageView07" android:id="@+id/TextView09"
				android:text="@string/help_main_para9"></TextView>
			<ImageView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:src="@drawable/line_portrait"
				android:layout_below="@+id/TextView09" android:id="@+id/ImageView08"></ImageView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textSize="@dimen/twozero_sp"
				android:textColor="@color/white" android:layout_below="@+id/ImageView08"
				android:id="@+id/TextView10" android:text="@string/help_main_para10"
				android:textStyle="bold" android:layout_centerInParent="true"
				android:paddingBottom="0px" android:paddingTop="10px"></TextView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textSize="@dimen/twozero_sp"
				android:textColor="@color/white" android:layout_below="@+id/TextView10"
				android:id="@+id/TextView11" android:text="@string/help_main_para11"
				android:textStyle="bold" android:paddingLeft="10px"
				android:paddingRight="10px" android:paddingTop="10px"></TextView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textSize="@dimen/twozero_sp"
				android:textColor="@color/white" android:text="@string/help_main_para12"
				android:id="@+id/TextView12" android:layout_below="@+id/TextView11"
				android:paddingLeft="10px" android:paddingRight="10px"
				android:paddingBottom="10px"></TextView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textSize="@dimen/twozero_sp"
				android:layout_below="@+id/TextView12" android:id="@+id/TextView13"
				android:text="@string/help_main_para13" android:textStyle="italic"
				android:textColor="@color/yellow" android:paddingTop="10px"
				android:paddingBottom="10px" android:layout_centerHorizontal="true"></TextView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textSize="@dimen/twozero_sp"
				android:textColor="@color/white" android:paddingLeft="10px"
				android:paddingRight="10px" android:layout_below="@+id/TextView13"
				android:id="@+id/TextView14" android:paddingTop="10px"
				android:textStyle="bold" android:text="@string/help_main_para14"></TextView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textSize="@dimen/twozero_sp"
				android:textColor="@color/white" android:paddingLeft="10px"
				android:paddingRight="10px" android:layout_below="@+id/TextView14"
				android:id="@+id/TextView15" android:paddingBottom="10px"
				android:text="@string/help_main_para15"></TextView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textSize="@dimen/twozero_sp"
				android:textStyle="italic" android:textColor="@color/yellow"
				android:paddingTop="10px" android:paddingBottom="10px"
				android:layout_below="@+id/TextView15" android:id="@+id/TextView16"
				android:text="@string/help_main_para16"
				android:layout_centerHorizontal="true"></TextView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textSize="@dimen/twozero_sp"
				android:textColor="@color/white" android:paddingLeft="10px"
				android:paddingRight="10px" android:layout_below="@+id/TextView16"
				android:id="@+id/TextView17" android:paddingTop="10px"
				android:text="@string/help_main_para17" android:textStyle="bold"></TextView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textSize="@dimen/twozero_sp"
				android:textColor="@color/white" android:paddingLeft="10px"
				android:paddingRight="10px" android:layout_below="@+id/TextView17"
				android:id="@+id/TextView18" android:paddingBottom="10px"
				android:text="@string/help_main_para18"></TextView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textSize="@dimen/twozero_sp"
				android:textStyle="italic" android:textColor="@color/yellow"
				android:paddingTop="10px" android:paddingBottom="10px"
				android:layout_below="@+id/TextView18" android:id="@+id/TextView19"
				android:text="@string/help_main_para19"
				android:layout_centerHorizontal="true"></TextView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textSize="@dimen/twozero_sp"
				android:textColor="@color/white" android:paddingLeft="10px"
				android:paddingRight="10px" android:layout_below="@+id/TextView19"
				android:id="@+id/TextView20" android:paddingTop="10px"
				android:textStyle="bold" android:text="@string/help_main_para20"></TextView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textSize="@dimen/twozero_sp"
				android:textColor="@color/white" android:paddingLeft="10px"
				android:paddingRight="10px" android:layout_below="@+id/TextView20"
				android:id="@+id/TextView21" android:paddingBottom="10px"
				android:text="@string/help_main_para21"></TextView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textSize="@dimen/twozero_sp"
				android:textStyle="italic" android:textColor="@color/yellow"
				android:paddingTop="10px" android:paddingBottom="10px"
				android:layout_centerHorizontal="true" android:layout_below="@+id/TextView21"
				android:id="@+id/TextView22" android:text="@string/help_main_para22"></TextView>
		</RelativeLayout>
	</ScrollView>
</RelativeLayout>
